# Some helper script functions.  Unfortunately, we cannot use  kinds
# of arugments here because of the m4 substitution.  So we have to set
# special variable names before invoking the function.  :-\

pmix_show_title() {
  cat <<EOF

============================================================================
== ${1}
============================================================================
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:2927: === ${1}" >&5
else
    echo === ${1} >&5
fi
}


pmix_show_subtitle() {
  cat <<EOF

*** ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:2943: *** ${1}" >&5
else
    echo *** ${1} >&5
fi
}


pmix_show_subsubtitle() {
  cat <<EOF

+++ ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:2959: +++ ${1}" >&5
else
    echo +++ ${1} >&5
fi
}

pmix_show_subsubsubtitle() {
  cat <<EOF

--- ${1}
EOF

# 1 is the message
# 2 is whether to put a prefix or not
if test -n "1"; then
    echo "configure:2974: --- ${1}" >&5
else
    echo --- ${1} >&5
fi
}

